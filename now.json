{
    "version": 2,
    "builds": [{ "src": "server.js", "use": "@now/node-server" }],
    "routes": [
        {
            "src": "/",
            "dest": "/server.js",
            "methods": ["GET"]
          },
      {
        "src": "/movies",
        "dest": "/server.js",
        "methods": ["GET"]
      },
      {
        "src": "/:id",
        "dest": "/server.js",
        "methods": ["GET"]
      },
      {
        "src": "/movie/:id",
        "dest": "/server.js",
        "methods": ["GET"]
      },{
        "src": "/movie",
        "dest": "/server.js",
        "methods": ["GET"]
      },
      {
        "src": "/movie/page/:id",
        "dest": "/server.js",
        "methods": ["GET"]
      }
    ]
  }